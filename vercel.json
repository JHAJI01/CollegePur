{
  "version": 2,
  "buildCommand": "cd Internship && npm run build",
  "outputDirectory": "Internship/dist",
  "framework": "vite",
  "cleanUrls": true,
  "trailingSlash": false,
  "github": {
    "silent": true
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "accept-ranges",
          "value": "bytes"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|json|txt|html|ico|svg)$",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "accept-ranges",
          "value": "bytes"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "x-content-type-options",
          "value": "nosniff"
        },
        {
          "key": "x-frame-options",
          "value": "DENY"
        },
        {
          "key": "x-xss-protection",
          "value": "1; mode=block"
        },
        {
          "key": "referrer-policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "permissions-policy",
          "value": "camera=(), microphone=(), geolocation=()"
        },
        {
          "key": "strict-transport-security",
          "value": "max-age=31536000; includeSubDomains"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 